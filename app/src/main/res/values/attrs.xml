<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextContent">
        <attr name="typeface" format="enum">
            <enum name="black" value="0" />
            <enum name="black_italic" value="1" />
            <enum name="bold" value="2" />
            <enum name="bold_italic" value="3" />
            <enum name="italic" value="4" />
            <enum name="light" value="5" />
            <enum name="light_italic" value="6" />
            <enum name="medium" value="7" />
            <enum name="medium_italic" value="8" />
            <enum name="regular" value="9" />
            <enum name="thin" value="10" />
            <enum name="thin_italic" value="11" />
        </attr>

        <attr name="underlined" format="boolean" />
    </declare-styleable>
</resources>